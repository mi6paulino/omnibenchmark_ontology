PREFIX schema: <http://schema.org/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX renku: <https://swissdatasciencecenter.github.io/renku-ontology#>
PREFIX omni: <http://omnibenchmark.org/ns#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

select ?module ?deliverable ?ci ?fn ?hash ?epoch
where {
 	    ?module a omni:Module .
    	?deliverable a omni:Deliverable .
  	    ?module omni:ciCdStage ?ci .
  	    ?deliverable omni:fileName ?fn .
  	    ?deliverable omni:lfsHash ?hash .
        ?deliverable omni:epoch ?epoch .
        FILTER (?epoch="epoch1"^^xsd:string) 
}


